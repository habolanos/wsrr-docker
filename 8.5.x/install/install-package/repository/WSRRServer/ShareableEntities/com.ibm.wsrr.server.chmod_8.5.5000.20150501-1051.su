<?xml version='1.0' encoding='UTF-8'?>
<?metadata version='0.0.4'?>
<su id='com.ibm.wsrr.server.chmod' version='8.5.5000.20150501-1051' iuCount='5'>
  <selector id='main'/>
  <iu id='wsrr.server.chmod.unix' version='8.5.5000.20150501-1051' adapterId='native'>
    <selectedByExpression>
      <and>
        <selectedBy id='main'/>
        <not>
          <selectedBy id='os' value='win32'/>
        </not>
      </and>
    </selectedByExpression>
    <adapterSpecificData>
      <perform duringPhases='install'>
        <chmod perm='a+x' recursive='false'>
          <file>${profile:installLocation}/bin/wsrr_ivt.sh</file>
          <file>${profile:installLocation}/WSRR/admin/ConfigureUDDI.sh</file>
          <file>${profile:installLocation}/WSRR/upgrade/upgrade.sh</file>
          <file>${profile:installLocation}/WSRR/CLI/wsrrcli_zos.sh</file>
          <file>${profile:installLocation}/WSRR/CLI/wsrrcli.sh</file>
          <file>${profile:installLocation}/WSRR/dbscripts/db2/configureDb2.sh</file>
          <file>${profile:installLocation}/WSRR/dbscripts/db2/runstats.sh</file>
          <file>${profile:installLocation}/WSRR/install/zWSRRInstall.sh</file>
          <file>${profile:installLocation}/WSRR/install/checkdb2.sh</file>
          <file>${profile:installLocation}/profileTemplates/default.wsrr/actions/scripts/copyNonConfigFiles.sh</file>
          <file>${profile:installLocation}/profileTemplates/default.wsrr/actions/scripts/updateNodeMetadata.sh</file>
          <file>${profile:installLocation}/profileTemplates/default.wsrr/actions/scripts/copyNonConfigFiles.sh</file>
          <file>${profile:installLocation}/profileTemplates/default.wsrr/actions/scripts/updateNodeMetadata.sh</file>
          <file>${profile:installLocation}/profileTemplates/default.wsrr/actions/scripts/copyNonConfigFiles.sh</file>
          <file>${profile:installLocation}/firststeps/wsrr/launchpad.sh</file>
          <file>${profile:installLocation}/firststeps/wsrr/launchpad/CheckShell.sh</file>
          <file>${profile:installLocation}/firststeps/wsrr/launchpad/Firefox_bash.sh</file>
          <file>${profile:installLocation}/firststeps/wsrr/launchpad/Firefox.sh</file>
          <file>${profile:installLocation}/firststeps/wsrr/launchpad/GetLocale.sh</file>
          <file>${profile:installLocation}/firststeps/wsrr/launchpad/Mozilla_bash.sh</file>
          <file>${profile:installLocation}/firststeps/wsrr/launchpad/Mozilla.sh</file>
          <file>${profile:installLocation}/firststeps/wsrr/launchpad/NoBrowser.sh</file>
          <file>${profile:installLocation}/firststeps/wsrr/launchpad/SeaMonkey_bash.sh</file>
          <file>${profile:installLocation}/firststeps/wsrr/launchpad/SeaMonkey.sh</file>
          <file>${profile:installLocation}/firststeps/wsrr/launchpad/SetArchitecture.sh</file>
          <file>${profile:installLocation}/firststeps/wsrr/launchpad/unittest.sh</file>
        </chmod>
      </perform>
    </adapterSpecificData>
  </iu>
  <iu id='wsrr.server.core.ant' version='8.5.5000.20150501-1051' adapterId='ant'>
    <selectedByExpression>
      <and>
        <selectedBy id='main'/>
        <or>
          <selectedBy id='os' value='win32'/>
          <selectedBy id='os' value='linux'/>
        </or>
      </and>
    </selectedByExpression>
    <adapterSpecificData>
      <perform duringPhases='post_install_configure'>
        <mkdir
          dir='${profile:installLocation}/logs/postinstall'/>
        <ant
          antfile='${profile:installLocation}/WSRR/install/CreateShortcutsWSRR.ant'
          workingDirectory='${profile:installLocation}'
          description='Start menu shortcuts'
          target='createProductShortCut2StartMenuWSRR'
          output='${profile:installLocation}/logs/postinstall/CreateShortcutsWSRR.log'
          loglevel='debug'>
          <property
            name='WAS_HOME'
            value='${profile:installLocation}'/>
        </ant>
      </perform>
      <perform duringPhases='pre_uninstall_configure'>
        <ant
          antfile='${profile:installLocation}/WSRR/install/DeleteShortcutsWSRR.ant'
          workingDirectory='${profile:installLocation}'
          description='Start menu shortcuts'
          target='deleteProductShortCutStartMenuWSRR'
          output='${profile:installLocation}/logs/postinstall/DeleteShortcutsWSRR.log'
          loglevel='debug'>
          <property
            name='WAS_HOME'
            value='${profile:installLocation}'/>
        </ant>
      </perform>
    </adapterSpecificData>
  </iu>
  <iu id='wsrr.server.profile.responsefiles' version='8.5.5000.20150501-1051' adapterId='ant'>
    <selectedBy id='main'/>
    <adapterSpecificData>
      <perform duringPhases='post_install_configure'>
        <mkdir
          dir='${profile:installLocation}/logs/postinstall'/>
        <ant
          antfile='${profile:installLocation}/WSRR/install/configureResponseFilesWSRR.ant'
          workingDirectory='${profile:installLocation}'
          description='Update sample manageprofiles response files'
          target='configureResponseFilesWSRR'
          output='${profile:installLocation}/logs/postinstall/configureResponseFilesWSRR.log'
          loglevel='debug'>
          <property
            name='WAS_HOME'
            value='${profile:installLocation}'/>
        </ant>
      </perform>
    </adapterSpecificData>
  </iu>
  <iu id='wsrr.server.profile.maskPasswords' version='8.5.5000.20150501-1051' adapterId='ant'>
    <selectedBy id='main'/>
    <adapterSpecificData>
      <perform duringPhases='post_install_configure'>
        <mkdir
          dir='${profile:installLocation}/logs/postinstall'/>
        <ant
          antfile='${profile:installLocation}/WSRR/install/maskPasswords.ant'
          workingDirectory='${profile:installLocation}'
          description='Add password fields to WAS mask'
          target='main'
          output='${profile:installLocation}/logs/postinstall/maskPasswordsWSRR.log'
          loglevel='debug'>
          <property
            name='WAS_HOME'
            value='${profile:installLocation}'/>
        </ant>
      </perform>
    </adapterSpecificData>
  </iu>
  <iu id='wsrr.server.upgrade.ant' version='8.5.5000.20150501-1051' adapterId='ant'>
    <selectedBy id='main'/>
    <adapterSpecificData>
      <perform duringPhases='post_install_configure'>
        <mkdir
          dir='${profile:installLocation}/logs/postinstall'/>
        <ant
          antfile='${profile:installLocation}/WSRR/install/WSRRProfiles.ant'
          workingDirectory='${profile:installLocation}'
          description='Profile Upgrade'
          target='default'
          output='${profile:installLocation}/logs/postinstall/WSRRProfiles.log'
          loglevel='debug'>
          <property
            name='was.installdir'
            value='${profile:installLocation}'/>
          <property
            name='DISABLEWSRRUPDATE'
            value='${environment:DISABLEWSRRUPDATE}'/>
        </ant>
      </perform>
    </adapterSpecificData>
  </iu>
</su>
